{
  "version": "1.0.0",
  "name": "contact-management",
  "description": "Simple CRUD application with Node js as backend and React js as frontend. Developed for further improving and solidifying knowledge learned during the session conducted by LeapFrog Technology.",
  "private": true,
  "scripts": {
    "start": "npm run dev --prefix backend",
    "client": "npm run start --prefix frontend",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "module:all": "npm i && cd backend && npm i && cd .. && cd frontend && npm i && cd ..",
    "doc": "jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "engines": {
    "node": ">= 14.17.6",
    "npm": ">= 6.14.15"
  },
  "author": {
    "name": "Sarwesh Maharjan",
    "email": "sarwesh.maharjan@gmail.com",
    "url": "https://github.com/sarweshmaharjan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarweshmaharjan/lf-contact-manager.git"
  },
  "keywords": [
    "contact",
    "react",
    "node",
    "express",
    "mongodb",
    "management"
  ],
